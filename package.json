{
  "name": "ant-design-pro",
  "version": "4.5.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:prod": "cross-env REACT_APP_ENV=prod UMI_ENV=dev umi dev",
    "start:dev_china": "cross-env REACT_APP_ENV=dev DEPLOY_TARGET=cn MOCK=none UMI_ENV=dev umi dev",
    "start:prod_china": "cross-env REACT_APP_ENV=prod DEPLOY_TARGET=cn UMI_ENV=dev umi dev",
    "build:dev": "node fixumi.js && REACT_APP_ENV=dev umi build",
    "build:prod": "node fixumi.js && REACT_APP_ENV=prod umi build",
    "build:dev_china": "node fixumi.js && REACT_APP_ENV=dev DEPLOY_TARGET=cn umi build",
    "build:prod_china": "node fixumi.js && REACT_APP_ENV=prod DEPLOY_TARGET=cn umi build",
    "locales:dev": "node download-locales --env=dev",
    "locales:prd": "node download-locales --env=prd",
    "analyze": "cross-env ANALYZE=1 umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "dev:https": "npm run start:https",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:https": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev HTTPS=1 umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-descriptions": "^1.2.0",
    "@ant-design/pro-form": "^1.3.0",
    "@ant-design/pro-layout": "^6.9.0",
    "@ant-design/pro-table": "^2.17.0",
    "@fingerprintjs/fingerprintjs": "^3.3.0",
    "@umijs/route-utils": "^1.0.33",
    "ahooks": "^3.3.12",
    "antd": "^4.12.0",
    "antd-mobile": "^5.18.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "blueimp-md5": "^2.19.0",
    "callapp-lib": "^3.5.1",
    "classnames": "^2.2.6",
    "clipboardy": "^3.0.0",
    "dayjs": "^1.11.5",
    "decimal.js-light": "^2.5.1",
    "dplayer": "^1.26.0",
    "easy-react-captcha": "^1.0.0",
    "echarts": "^5.3.1",
    "echarts-for-react": "^3.0.2",
    "events": "^3.3.0",
    "firebase": "^9.6.1",
    "hls.js": "^1.0.4",
    "idb-keyval": "^6.0.3",
    "install": "^0.13.0",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.33",
    "npm": "^7.11.2",
    "omit.js": "^2.0.2",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-px2rem": "^0.3.0",
    "qrcode.react": "^3.1.0",
    "query-string": "^7.0.1",
    "react": "^16.14.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-countdown": "^2.3.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-helmet-async": "^1.0.4",
    "react-iframe": "^1.8.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-page-scroller": "^2.2.1",
    "react-sortablejs": "^6.0.0",
    "react-sticky": "^6.0.3",
    "react-transition-group": "^4.4.2",
    "react-weui": "^1.2.3",
    "sortablejs": "^1.13.0",
    "swiper": "^7.4.1",
    "umi": "3.5.24",
    "umi-plugin-keep-alive": "^0.0.1-beta.30",
    "umi-request": "^1.0.8",
    "url-parse": "^1.5.10",
    "vconsole": "^3.8.1",
    "wangeditor": "^4.7.15",
    "weui": "^1.1.3"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.28",
    "@types/blueimp-md5": "^2.18.0",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/moment-timezone": "^0.5.30",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/sortablejs": "^1.10.6",
    "@types/url-parse": "^1.4.8",
    "@umijs/fabric": "^2.5.1",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "puppeteer-core": "^8.0.0",
    "px2rem-loader": "^0.1.9",
    "stylelint": "^13.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
